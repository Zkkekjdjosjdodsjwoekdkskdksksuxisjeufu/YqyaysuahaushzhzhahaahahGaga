local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()

local Window = OrionLib:MakeWindow({Name = "1clan Premium Script", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})

local Tab = Window:MakeTab({
    Name = "Reach",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

-- Bu alttaki kisim
Tab:AddButton({
    Name = "Reach gui",
    Callback = function()
              getgenv().Kitten = 5
loadstring(game:HttpGet("https://pastebin.com/raw/EdDK6iWA",true))()
      end    
})

