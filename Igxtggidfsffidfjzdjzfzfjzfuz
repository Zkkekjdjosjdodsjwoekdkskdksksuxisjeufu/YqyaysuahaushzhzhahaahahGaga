for i,v in pairs(game.Players:GetPlayers()) do
    if v.Name ~= game.Players.LocalPlayer.Name then
        for i,b in pairs(v.Character.HumanoidRootPart:GetDescendants()) do
            if b.Name == "Fire" or b.Name == "HitScript" then
                b:Destroy()
            end
        end
end
